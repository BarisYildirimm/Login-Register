

<div class="card">
    <div class="card-header">Registered Users</div>
    <div class="card-body">
        <ul class="list-group">
            {{#each users}}
            <li class="list-group-item">{{username}}</li>
            {{else}}
            <div class="alert alert-info">
                There is no registered Users!
            </div>
            {{/each}}
        </ul>
    </div>
</div>